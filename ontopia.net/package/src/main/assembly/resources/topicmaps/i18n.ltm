[script-category  = "Script category"
 @"http://psi.ontopia.net/i18n/#category"]
{script-category, definition, [[The broadest category of scripts,
reflecting how the script came into being and what it has been used
for, rather than being typology based on their features.]]}

[script-group  = "Script group"
 @"http://psi.ontopia.net/i18n/#group"]
{script-group, article-about, "http://www.dlib.org/dlib/march97/sesame/03clews.html"}
{script-group, definition, [[The broadest category of natural scripts,
reflecting the historical development of scripts, rather than being a
typology based on their features.]]}

[script-family = "Script family"
 @"http://psi.ontopia.net/i18n/#family"]
{script-family, definition, [[A category of scripts derived from the
same source. Usually the scripts in a script family are closely
related, both historically and in features.]]}

[transcription = "Transcription"
 @"http://psi.ontopia.net/i18n/#transcription"]
{transcription, definition, [[A standardized method for representing
the sound of one language in a script not normally used to write that
language.]]}
{transcription, article-about, "http://www.xrce.xerox.com/competencies/content-analysis/arabic/info/romanization.html"}

[transliteration = "Transliteration"
 @"http://psi.ontopia.net/i18n/#transliteration"]
{transliteration, definition, [[A standardized method for representing
an ortography of a language in a script different from the one the
ortography is actually written in.]]}
{transliteration, article-about, "http://www.xrce.xerox.com/competencies/content-analysis/arabic/info/romanization.html"}

